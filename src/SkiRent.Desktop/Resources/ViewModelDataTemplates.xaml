<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:adminVM="clr-namespace:SkiRent.Desktop.ViewModels.Admin"
                    xmlns:admin="clr-namespace:SkiRent.Desktop.Views.Admin"
                    xmlns:mainVM="clr-namespace:SkiRent.Desktop.ViewModels.Main"
                    xmlns:main="clr-namespace:SkiRent.Desktop.Views.Main">

    <DataTemplate DataType="{x:Type mainVM:MainViewModel}">
        <main:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type adminVM:AdminMainViewModel}">
        <admin:AdminMainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type adminVM:AdminMenuViewModel}">
        <admin:AdminMenu />
    </DataTemplate>

</ResourceDictionary>
